{
  "name": "basic",
  "manifest_version": 2,
  "version": "1.0",
  "description": "basic",
  "browser_specific_settings": {
    "gecko": {
      "id": "basic@search.mozilla.org"
    }
  },
  "hidden": true,
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "basic",
      "keyword": "@basic",
